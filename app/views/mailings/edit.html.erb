<h2>Edit Mailing</h2>

<%= form_for @mailing, html: {multipart: true} do |f| %>
  <%= render partial: "form", locals: { f: f } %>

  <div class="row mt-3">
    <div class="col-sm-3">
      <button type="submit" class="btn btn-primary">Update</button> | <%= link_to 'Back', mailing_path(@mailing) %>
    </div>
  </div>
<% end %>
