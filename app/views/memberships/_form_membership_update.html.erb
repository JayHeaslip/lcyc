<div class="form-group row mb-1">
  <label for="MemberSince" class="col-form-label col-sm-2">Member Since</label>
  <div class="col-sm-10">
    <%= f.text_field :MemberSince, id: "MemberSince", class: "form-control" %>
  </div>
</div>
<div class="form-group row mb-1">
  <label for="Status" class="col-form-label col-sm-2">Status</label>
  <div class="col-sm-10">
    <%= f.collection_select :Status, Memberstatus.all, :Status, :Status, class: "form-control" %>
  </div>
</div>
