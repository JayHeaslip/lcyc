<h2>Generate Invoices</h2>
<%= form_tag generate_invoices_quickbooks_path do %>
<div class="form-row form-check mr-4 pb-2">
  <%= check_box_tag :test, :test, @test, class: "form-check-input" %>
  <label for="html" class="form-check-label">Test?</label>
</div>
<div class="form-group row mt-3">
  <div class="col-sm-3">
    <button type="submit" class="btn btn-primary">Generate Invoices</button>
  </div>
</div>
<% end %>
